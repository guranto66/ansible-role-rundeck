    debug="{{rundeck_ldap.debug}}"
    contextFactory="com.sun.jndi.ldap.LdapCtxFactory"
    providerUrl="{{rundeck_ldap.providerUrl}}"
    bindDn="{{rundeck_ldap.bindDn}}"
    bindPassword="{{rundeck_ldap.bindPassword}}"
